body {
  font-family: "Ubuntu", sans-serif;
  font-weight: 400;
  font-style: normal;
  color: #F5F5F5;
}

.center {
  background-color: #80054c;
  max-width: 1100px;
  margin: auto;
}

/* Create three equal columns that floats next to each other */
.column {
  float: left;
  width: 33.33%;
  /*padding: 10px;*/
  height: 300px; /* Should be removed. Only for demonstration */
} 


/* Clear floats after the columns */
.row:after {
  content: "";
  display: table;
  clear: both;
}

